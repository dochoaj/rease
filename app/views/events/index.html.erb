<% if user_signed_in? %>
	<%= link_to 'Nuevo evento', new_event_path , :id => "botonCRUD", :class => "btn btn-lg"%>
<% end %>
<div id="panelRease" class="panel panel-default">
	<div class="panel-heading">
		Calendario de eventos

	</div>
	<div class="panel-body" id = "calendar">
	</div>
</div>

<script type="text/javascript">
	$(document).ready(function() {
		$('#calendar').fullCalendar({
			events: '/events.json'
		})
	});
</script>


